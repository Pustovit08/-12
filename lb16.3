import tkinter as tk

root = tk.Tk()
root.title("Малювання фігур")

canvas = tk.Canvas(root, width=400, height=300, bg="white")
canvas.pack()

# Прямокутник
canvas.create_rectangle(50, 50, 150, 120, fill="skyblue", outline="black")

# Овал
canvas.create_oval(200, 100, 300, 180, fill="lightgreen", outline="darkgreen")

root.mainloop()
